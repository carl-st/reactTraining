{
    "extends": ["standard", "standard-react"],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "react/sort-prop-types": [1, {
            "callbacksLast": 1,
            "ignoreCase": 1,
            "requiredFirst": 1
        }]
    },
    "globals": {
        "expect": true,
        "test": true
    }
}
